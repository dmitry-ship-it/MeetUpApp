{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultDb": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=AppData;Integrated Security=True"
  },

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },

  "Identity": {
    "ApiScopes": [
      "MeetUpApp.Api"
    ],
    "ApiResourceName": "MeetUpApp.Api",
    "ClientId": "superuser",
    "ClientName": "MeetUpApp.Api",
    "ClientSecret": "secret"
  }
}
